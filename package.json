{
  "name": "online-ide",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "tsc && vite build && vite build --config vite.config-embedded.js",
    "build-ide": "tsc && vite build",
    "build-embedded": "tsc && vite build --config vite.config-embedded.js",
