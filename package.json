    "build_tools": "tsc --project tsconfig_development-tools.json"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.30",
    "@types/node": "22.1.0",
    "jimp": "^0.22.12",
    "monaco-editor": "^0.50.0",
    "spritesmith": "^3.4.1",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "@types/chart.js": "^2.9.41",
    "chalk": "^5.3.0",
    "jquery": "^3.6.3",
    "madge": "^8.0.0",
    "pixi.js": "8.3.0"
  }
}
